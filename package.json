{
  "name": "@diplodoc/pdf-generator",
  "version": "1.1.10",
  "description": "Package to turn Diplodoc projects into PDFs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/diplodoc-platform/pdf-generator"
  },
  "bin": {
    "pdf-generator": "build/cmd/index.js"
  },
  "main": "build/index.js",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "build": "webpack",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint": "npm run lint:js",
    "lint:fix": "eslint --fix ./src",
    "binary": "pkg .",
    "test": "npm run build && npm run integration:prepare && jest -i",
    "integration:clean": "rm -rf ./integration-output",
    "integration:build": "npx -y -- @diplodoc/cli@latest -i ./integration -o ./integration-output --pdf",
    "integration:prepare": "npm run integration:clean && npm run integration:build",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "dependencies": {
    "@diplodoc/transform": "^4.57.5",
    "async": "^3.2.4",
    "pdf-lib": "^1.17.1",
    "puppeteer-chromium-resolver": "^19.1.0",
    "puppeteer-core": "^19.5.2",
    "walk-sync": "^3.0.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-syntax-top-level-await": "7.14.5",
    "@babel/plugin-transform-runtime": "7.16.8",
    "@babel/preset-env": "7.16.8",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@babel/runtime": "7.16.7",
    "@gravity-ui/eslint-config": "^2.2.0",
    "@gravity-ui/prettier-config": "^1.1.0",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@types/async": "^3.2.15",
    "@types/jest": "^29.5.6",
    "@types/jest-image-snapshot": "^6.2.2",
    "@types/node": "^16.11.7",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.2.3",
    "css-loader": "^6.11.0",
    "eslint": "^8.32.0",
    "jest": "^29.7.0",
    "jest-image-snapshot": "^6.2.0",
    "pdf2pic": "^3.0.3",
    "pkg": "^5.8.1",
    "prettier": "^2.8.8",
    "puppeteer": "^24.8.2",
    "raw-loader": "^4.0.2",
    "sass": "^1.80.4",
    "sass-loader": "^16.0.2",
    "shelljs": "^0.8.5",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "pkg": {
    "scripts": [
      "build/cmd/index.js"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64"
    ],
    "outputPath": "dist-bin"
  }
}
